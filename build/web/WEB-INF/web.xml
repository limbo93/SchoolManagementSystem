<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>ServletController.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Logout</servlet-name>
        <servlet-class>ServletController.Admin_Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teacher_Registration_Controller</servlet-name>
        <servlet-class>ServletController.Teacher_Registration_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teacher_View_Controller</servlet-name>
        <servlet-class>ServletController.Teacher_View_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Student_Registration_Controller</servlet-name>
        <servlet-class>ServletController.Student_Registration_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teacher_Login_Controller</servlet-name>
        <servlet-class>ServletController.Teacher_Login_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Student_Login_Controller</servlet-name>
        <servlet-class>ServletController.Student_Login_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Student_Logout_Controller</servlet-name>
        <servlet-class>ServletController.Student_Logout_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teacher_Logout_Controller</servlet-name>
        <servlet-class>ServletController.Teacher_Logout_Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edit_Teacher_Controller</servlet-name>
        <servlet-class>ServletController.Edit_Teacher_Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Logout</servlet-name>
        <url-pattern>/Admin_Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teacher_Registration_Controller</servlet-name>
        <url-pattern>/Teacher_Registration_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teacher_View_Controller</servlet-name>
        <url-pattern>/Teacher_View_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Student_Registration_Controller</servlet-name>
        <url-pattern>/Student_Registration_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teacher_Login_Controller</servlet-name>
        <url-pattern>/Teacher_Login_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Student_Login_Controller</servlet-name>
        <url-pattern>/Student_Login_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Student_Logout_Controller</servlet-name>
        <url-pattern>/Student_Logout_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teacher_Logout_Controller</servlet-name>
        <url-pattern>/Teacher_Logout_Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edit_Teacher_Controller</servlet-name>
        <url-pattern>/Edit_Teacher_Controller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
